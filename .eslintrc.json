{
  "extends": "next/core-web-vitals",
  "plugins": ["import-helpers"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "import-helpers/order-imports": [
      "warn",
      {
        "newlinesBetween": "always",
        "groups": [
          "/^react/",
          "/^next/",
          "module",
          "/^@/",
          ["parent", "sibling", "index"]
        ],
        "alphabetize": {
          "order": "asc",
          "ignoreCase": true
        }
      }
    ]
  }
}
